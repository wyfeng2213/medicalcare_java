    <script type="text/javascript">
        $(".chat-item ").click(function () {
            $(".chat-item ").removeClass("active");
            $(this).addClass("active")
            var index = $(this).index()
            $(".chat-content-show-area").css({ "display": "none" });
            $(".chat-content-show-area").eq(index).css({ "display": "block" })
            console.log($(this).index())
        });

        $(".operation li").click(function () {
            var data = $(this).find("div").attr("data");
            $("iframe").css("display", "block")
            $("iframe").attr("src", data)
        })

        for (var i = 1; i <= 75; i++) {

            $(".face").append('<img src="../../Content/face/' + i + '.gif">')

        };
        //调用表情窗口    关闭表情窗口
        $(".web_wechat_face ").click(function () {
            $(".face").fadeToggle(300)

        })
        $(".face img").click(function () {
            $(".face").fadeOut(600);

            var clone = $(this).clone(true)
            var src = $(this).attr("src")

            $('.take-content-area').append('<div class="answer-question ss">'
                + '<img src="../../Content/images/people.jpg" alt="">'
                + '<p><span class="span-left"></span>' + '<img src="' + src + '">' + '</p></div>')
        })



