body{font-size:12px;  background: #f0f4f9;}
#page-wrap {
    font-size: 0.15rem;
    background: #f0f4f9;
    color: #666;
    min-height:auto;
}
.list-user-info {
    padding: 0 0 0 0.22rem;
    background: #fff;
}
.form-user-infos li, .form-user-infos .user-info {
    height: 0.66rem;
    line-height: 0.66rem;
    border-bottom: 0.01rem solid #f0f4f9;
    padding: 0 0.18rem 0 0;
}
.form-user-infos label {
    display: inline-block;
    width: 0.65rem;
    height: 100%;
}
.form-user-infos input {
    display: inline-block;
    float: right;
    width: 2.6rem;
    height: 100%;
    line-height: 0.66rem;
    margin: 0;
    padding: 0;
    text-align: right;
    border: 0;
}
.btn-apply {
    display: block;
    margin: 0.6rem auto 0;
    background: #358bf7;
    color: #fff;
    line-height: inherit;
    font-size: 0.18rem;
}
.history{
    height:0.5rem;
    line-height:0.5rem;
    font-size:0.16rem;
    position:fixed;
    top:0;
    left:0;width:100%;background:#555;color:#fff;
    text-align:center;

}
.history i{position:absolute;left:0.2rem;font-size: 0.22rem;}
.content-warp{margin-top:0.5rem}
.content-warp .title{line-height:0.3rem;height:0.3rem;margin-left:0.22rem;font-size: 14px;}
