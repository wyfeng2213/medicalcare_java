body{font-size:14px;  background: #f0f4f9;    color: #333;}
.history{height:0.5rem; line-height:0.5rem;font-size:0.16rem; position:fixed; top:0; left:0;width:100%;background:#555;color:#fff; text-align:center;}
.history i{position:absolute;left:0.2rem;font-size: 0.22rem;}
.content-warp {margin-top: 0.5rem;}
.content-myWarp {margin-top: 0.2rem;}
.content-warp .item{margin-bottom:0.15rem;background:#fff;padding:0 0.22rem;font-size:0.14rem;line-height:1.4}
.notice{margin-bottom:0.15rem;background:#fff;padding:0 0.22rem;font-size:0.14rem;line-height:1.4}
.content-warp .item h3{font-size:0.16rem;height:0.35rem;line-height:0.35rem}
.content-warp .item .item-content{height:1.2rem;background:#bbb}
.content-warp .item p{line-height:1.4;font-size:0.14rem;    padding-bottom: 0.18rem;}
.content-warp .item .tip{padding:0.18rem 0;}
.apply-success i { position: relative; display: inline-block; width: 0.25rem; height: 0.25rem; background: #208ffb; color: #fff; border-radius: 50%;font-size: 0.16rem;
    vertical-align: middle;line-height: 0.25rem;text-align: center;margin: 0 0.04rem 0 0.2rem;}
.btn-apply {display: block;margin: 0.2rem auto 0.2rem;background: #358bf7; color: #fff; line-height: inherit; font-size: 0.18rem;}
.apply-success{width:90%;margin:0.2rem auto 0;font-size:0.13rem}
.apply-success a{display:inline-block;font-size:0.13rem}
dd{text-indent:1em;margin-bottom: 0.05rem;}